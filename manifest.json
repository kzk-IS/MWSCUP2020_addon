{

  "description": "Demonstrating webRequests",
  "manifest_version": 2,
  "name": "ELM Blocker",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "web_accessible_resources" : ["src/alert.html"],
  "background": {
    "scripts": ["./src/search.js","./src/tmpWhitelist.js","background.js"]
  }
}
